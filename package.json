{
  "name": "clue",
  "version": "0.0.1",
  "license": "SEE LICENSE IN LICENSE",
  "repository": "Caleydo/clue",
  "dependencies": {
  },
  "peerDependencies": {
    "caleydo_core": "*",
    "caleydo_provenance": "*",
    "caleydo_window": "*",
    "caleydo_vis": "*",
    "wrapper_bootstrap_fontawesome": "*"
  },
  "caleydo": {
    "plugins": {
      "web": [
        {
          "type": "application",
          "name": "CLUE"
        }
      ],
      "dependencies": {
        "web": {
          "d3": "~3.5.5",
          "graphlib": "~1.0.1",
          "dagre": "~0.7.1",
          "dagre-d3": "~0.4.3"
        },
        "tsd": {
          "dagre": "04d87e"
        }
      },
      "requirejs-config": {
        "paths": {
          "lodash": "${baseUrl}/lodash/dist/lodash",
          "graphlib": "${baseUrl}/graphlib/dist/graphlib.core",
          "dagre": "${baseUrl}/dagre/dist/dagre.core"
        },
        "shim": {
          "graphlib": {
            "deps": [
              "lodash"
            ],
            "exports": "graphlib"
          },
          "dagre": {
            "deps": [
              "lodash",
              "graphlib"
            ],
            "exports": "dagre"
          }
        }
      }
    }
  },
  "publishConfig": {
    "registry": "http://registry.caleydo.org/"
  }
}
