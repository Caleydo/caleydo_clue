{
  "name": "phovea_clue",
  "description": "The foundation for Vistories: CLUE playground for provenance storage and visualization",
  "version": "5.0.1-SNAPSHOT",
  "author": {
    "name": "The Caleydo Team",
    "email": "contact@caleydo.org",
    "url": "https://caleydo.org"
  },
  "license": "BSD-3-Clause",
  "homepage": "http://vistories.org",
  "bugs": {
    "url": "https://github.com/phovea/phovea_clue/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/phovea/phovea_clue.git"
  },
  "files": [
    "src",
    "index.js",
    "phovea.js",
    "phovea_registry.js",
    "build",
    "phovea_clue",
    "__init__.py",
    "__main__.py",
    "requirements.txt",
    "requirements_dev.txt",
    "docker_packages.txt",
    "redhat_packages.txt"
  ],
  "scripts": {
    "clean": "rimraf build dist && npm run clean:compile",
    "clean:compile": "rimraf src/**/*.map src/**/*.js src/**/*.d.ts tests/**/*.js tests/**/*.map tests/**/*.d.ts demo/**/*.js demo/**/*.map demo/**/*.d.ts",
    "check": "flake8 --exclude=.git,venv,deploy,docs,__pycache__,node_modules",
    "pretest": "echo hybrid",
    "test": "npm run test:web && npm run test:python",
    "predist": "echo hybrid",
    "dist": "npm run dist:web && npm run dist:python",
    "compile": "tsc",
    "lint": "tslint -c tslint.json -p . 'src/**/*.ts?(x)' 'tests/**/*.ts?(x)'",
    "docs": "npm run docs:web && npm run docs:python",
    "prebuild": "echo hybrid",
    "posttest": "echo hybrid",
    "test:watch": "jest --watch",
    "build:dev": "webpack",
    "build": "npm run build:web && npm run build:python",
    "start": "webpack-dev-server --inline",
    "start:hot": "webpack-dev-server --inline --hot",
    "watch": "webpack --watch",
    "release:major": "npm version major && npm publish && git push --follow-tags",
    "release:minor": "npm version minor && npm publish && git push --follow-tags",
    "release:patch": "npm version patch && npm publish && git push --follow-tags",
    "pretest:web": "npm run compile",
    "pretest:python": "npm run check",
    "posttest:web": "npm run lint",
    "test:web": "test ! $(find tests -name \"*.ts\") || jest",
    "test:python": "test ! $(find tests -name \"*.py\") || python setup.py test",
    "predist:web": "npm run build:web && npm run docs:web",
    "dist:web": "mkdir dist && cd build && tar cvzf ../dist/phovea_clue.tar.gz *",
    "dist:python": "python setup.py sdist bdist_wheel",
    "docs:web": "typedoc --options typedoc.json src/**.ts",
    "docs:python": "sphinx-apidoc -o docs -f ./phovea_clue && sphinx-build ./docs build/docs",
    "build:python": "rm -rf build/source && find . -name '*.pyc' -delete && node buildPython.js && cp -r ./phovea_clue build/source/",
    "build:web": "webpack --env prod",
    "predist:python": "npm run build:python && npm run docs:python",
    "prebuild:python": "node -e \"process.exit(process.env.PHOVEA_SKIP_TESTS === undefined?1:0)\" || npm run test:python",
    "prebuild:web": "node -e \"process.exit(process.env.PHOVEA_SKIP_TESTS === undefined?1:0)\" || npm run test:web",
    "publish:python": "twine upload --repository-url https://upload.pypi.org/legacy/ dist/*"
  },
  "main": "build/phovea_clue.js",
  "engines": {
    "npm": ">= 6.12",
    "node": ">= 12.13"
  },
  "dependencies": {
    "@types/d3": "~3.5.36",
    "@types/marked": "0.7.2",
    "d3": "~3.5.17",
    "marked": "0.7.0",
    "phovea_core": "github:phovea/phovea_core#stoiber/testing_with_jest",
    "phovea_security_flask": "github:phovea/phovea_security_flask#stoiber/testing_with_jest",
    "phovea_ui": "github:phovea/phovea_ui#stoiber/testing_with_jest"
  },
  "devDependencies": {
    "@types/jest": "23.3.13",
    "awesome-typescript-loader": "3.1.2",
    "cache-loader": "1.2.0",
    "css-loader": "0.28.0",
    "extract-text-webpack-plugin": "2.1.0",
    "extract-loader": "0.1.0",
    "identity-obj-proxy": "^3.0.0",
    "ifdef-loader": "2.0.0",
    "file-loader": "0.11.1",
    "fork-ts-checker-webpack-plugin": "0.4.4",
    "html-loader": "0.4.5",
    "imports-loader": "0.7.1",
    "jest": "^22.4.0",
    "jest-raw-loader": "1.0.1",
    "json-loader": "0.5.4",
    "node-sass": "^4.13.1",
    "null-loader": "0.1.1",
    "raw-loader": "0.5.1",
    "rimraf": "^3.0.0",
    "sass-loader": "6.0.7",
    "style-loader": "0.16.1",
    "thread-loader": "1.1.2",
    "ts-loader": "4.0.1",
    "tslib": "~1.11.0",
    "tslint": "5.9.1",
    "ts-jest": "22.4.4",
    "typedoc": "~0.16.10",
    "typescript": "~3.8.2",
    "url-loader": "0.5.8",
    "webpack": "2.3.3",
    "webpack-dev-server": "2.4.2"
  }
}
