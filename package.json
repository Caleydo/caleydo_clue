{
  "name": "clue_demo",
  "version": "0.0.1",
  "license": "SEE LICENSE IN LICENSE",
  "repository": "Caleydo/clue_demo",
  "dependencies": {
  },
  "peerDependencies": {
    "caleydo_core": "*",
    "caleydo_d3": "*",
    "caleydo_provenance": "*",
    "caleydo_window": "*",
    "caleydo_vis": "*",
    "caleydo_screenshot": "*",
    "wrapper_bootstrap_fontawesome": "*"
  },
  "caleydo": {
    "plugins": {
      "web": [
        {
          "type": "application",
          "name": "CLUE Demo"
        },
        {
          "type": "actionFactory",
          "id": "clue_demo",
          "file": "cmds",
          "factory": "createCmd",
          "creates": "(addClueElem|removeClueElem)"
        }
      ],
      "python": [
        {
          "type": "namespace",
          "id": "caleydo-clue-screenshot",
          "file": "remoteplayer",
          "namespace": "/api/clue",
          "factory": "create"
        }
      ]
    },
    "products": [
      {
        "name": "CLUE Demo",
        "type": "python",
        "app": "clue_demo",
        "plugins": [
          "clue_demo",
          "caleydo_server"
        ]
      }
    ],
    "dependencies": {
      "web": {
        "d3": "~3.5.5",
        "marked": "~0.3.3"
      },
      "tsd": {
        "marked": "935bfa"
      },
      "python": {
        "selenium": "2.48.0"
      },
      "debian": {
        "phantomjs": ""
      }
    },
    "requirejs-config": {
      "paths": {
      },
      "shim": {
      }
    }
  },
  "publishConfig": {
    "registry": "http://registry.caleydo.org/"
  }
}
